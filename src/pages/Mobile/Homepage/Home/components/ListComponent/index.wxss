:global .ant-list-item-meta {
  background: #ffffff;
  margin: 10px 0;
  padding: 10px;
  border-radius: 8px;
}
.TitleRow {
  display: flex;
  justify-content: space-between;
  height: 22px;
  margin-bottom: 8px;
  line-height: 22px;
}
.TitleRow .Title {
  width: calc(100% - 100px);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.TitleRow .Title span {
  color: #333333;
  font-weight: bold;
  font-size: 14px;
}
.TitleRow .Title .Headlines {
  display: inline-block;
  width: 36px;
  height: 22px;
  margin-right: 6px;
  color: #fff;
  font-size: 12px;
  line-height: 22px;
  text-align: center;
  background: #ff4646;
  border-radius: 11px;
}
.TitleRow .TitleRight span {
  color: #999999;
  font-weight: 400;
  font-size: 12px;
}
.descRow {
  display: flex;
  justify-content: space-between;
  height: 17px;
  font-size: 12px;
  line-height: 17px;
}
.descRow .descleft {
  width: calc(100% - 100px);
  overflow: hidden;
  color: #999999;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.descRow .descleft .descleftInspan {
  padding: 0 8px;
}
.descRow .descright .gengduo {
  position: relative;
  top: -80%;
  color: #DDDDDD;
  font-size: 20px;
}
